platform :ios, '10.0'

target 'iOS-Architectures' do
  use_frameworks!
	pod 'Bolts-Swift', :git => 'https://github.com/BoltsFramework/Bolts-Swift'
	pod 'Alamofire'
    pod 'Marshal'
 	pod 'ErrorHandler', :git => 'https://github.com/Workable/swift-error-handler'
    pod 'ErrorHandler/Alamofire', :git => 'https://github.com/Workable/swift-error-handler'

  target 'iOS-ArchitecturesTests' do
    inherit! :search_paths
  end

  target 'iOS-ArchitecturesUITests' do
    inherit! :search_paths
  end

end
